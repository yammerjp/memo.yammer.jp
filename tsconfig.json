{
  "include": [
    "./src/**/*.ts"
  ],
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "strict": false,
    "typeRoots": [
       "node_modules/@types",
       "./src/type"
     ],
     "types": [ "node" ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
