{
  "name": "blog-starter-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "build:static": "next build",
    "start": "next start -p 8080",
    "lint": "next lint --dir src",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css,scss}'",
    "typecheck": "tsc",
    "build:gh-pages-tags": "rm -rf gh-pages/tags/articleTags.json && mkdir -p gh-pages/tags && node bin/articleTags.js",
    "build:gh-pages-words": "rm -rf gh-pages/tags/articleWords.json && mkdir -p gh-pages/tags  && node bin/articleWords.js",
    "build:gh-pages-gitlogs": "rm -rf gh-pages/gitlogs && mkdir -p gh-pages/content/posts && bash bin/gitlogs.sh",
    "build:gh-pages-gitlogs-canary": "rm -rf gh-pages/gitlogs.json &&  mkdir -p gh-pages &&node bin/gitlogs.js",
    "build:gh-pages": "npm run build:gh-pages-gitlogs && npm run build:gh-pages-tags && npm run build:gh-pages-words && npm run build:gh-pages-gitlogs-canary",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@types/xml-escape": "^1.1.0",
    "gray-matter": "4.0.3",
    "hast-util-from-html": "^2.0.3",
    "highlight.js": "^11.7.0",
    "kuromoji": "^0.1.2",
    "next": "^15.3.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-code-titles": "^0.1.2",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "retext-stringify": "^4.0.0",
    "strip-markdown": "^6.0.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "xml-escape": "^1.1.0"
  },
  "devDependencies": {
    "@pinecone-database/pinecone": "^6.1.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "8.10.0",
    "eslint-config-next": "^15.3.5",
    "eslint-config-prettier": "^10.1.5",
    "isomorphic-git": "^1.10.1",
    "jsdom": "^26.1.0",
    "openai": "^5.8.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "license": "MIT"
}
