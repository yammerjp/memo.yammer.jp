{
  "name": "blog-starter-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "build:static": "next build && next export",
    "start": "next start -p 8080",
    "lint": "next lint --dir src",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css,scss}'",
    "typecheck": "tsc",
    "build:gh-pages-tags": "rm -rf gh-pages/tags/articleTags.json && mkdir -p gh-pages/tags && node bin/articleTags.js",
    "build:gh-pages-words": "rm -rf gh-pages/tags/articleWords.json && mkdir -p gh-pages/tags  && node bin/articleWords.js",
    "build:gh-pages-gitlogs": "rm -rf gh-pages/gitlogs && mkdir -p gh-pages/content/posts && bash bin/gitlogs.sh",
    "build:gh-pages-gitlogs-canary": "rm -rf gh-pages/gitlogs.json &&  mkdir -p gh-pages &&node bin/gitlogs.js",
    "build:gh-pages": "npm run build:gh-pages-gitlogs && npm run build:gh-pages-tags && npm run build:gh-pages-words && npm run build:gh-pages-gitlogs-canary",
    "test": "jest"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/xml-escape": "^1.1.0",
    "axios": "^0.21.1",
    "classnames": "2.3.1",
    "date-fns": "2.22.1",
    "gray-matter": "4.0.3",
    "hast-util-from-html": "^1.0.1",
    "highlight.js": "^11.7.0",
    "kuromoji": "^0.1.2",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-shadow": "^19.0.2",
    "rehype-highlight": "^6.0.0",
    "rehype-stringify": "^9.0.3",
    "remark": "^13.0.0",
    "remark-code-titles": "^0.1.2",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-html": "^13.0.2",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "retext-stringify": "^3.1.0",
    "strip-markdown": "^4.0.0",
    "typescript": "^4.3.4",
    "unified": "^10.1.1",
    "xml-escape": "^1.1.0"
  },
  "devDependencies": {
    "@pinecone-database/pinecone": "^0.1.6",
    "@testing-library/react": "^12.1.2",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^15.12.3",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "autoprefixer": "^10.2.6",
    "babel-jest": "^27.3.1",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "isomorphic-git": "^1.10.1",
    "jest": "^27.3.1",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^1.0.0",
    "openai": "^3.2.1",
    "postcss": "^8.4.31",
    "prettier": "^2.5.1",
    "tailwindcss": "^2.2.1"
  },
  "license": "MIT"
}
